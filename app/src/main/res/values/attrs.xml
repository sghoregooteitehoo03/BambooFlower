<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingView">
        <attr name="visible" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="customButtonIcon" format="reference|integer"/>
        <attr name="customButtonText" format="reference|string"/>
    </declare-styleable>

    <declare-styleable name="CustomSettingView">
        <attr name="settingText" format="reference|string"/>
        <attr name="settingTextColor" format="reference|color"/>
        <attr name="settingIconVisible" format="reference|boolean"/>
        <attr name="settingSwitchVisible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomProgressView">
        <attr name="customSetProgress" format="integer"/>
        <attr name="customSetProgressText" format="string"/>
        <attr name="customSetSecondProgress" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomMenuView">
        <attr name="customMenuMainColor" format="color|integer"/>
        <attr name="customMenuSubColor" format="color|integer"/>
        <attr name="customMenuImage" format="reference|integer"/>
        <attr name="customMenuTitle" format="string"/>
    </declare-styleable>
</resources>